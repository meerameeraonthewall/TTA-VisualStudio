@model List<Entry>

<div class="row no-gutters m-3">
    <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
        <div class="card-header text-center">
            <h5 class="m-0">Quotes</h5>
        </div>

        <div class="card-body">
            @foreach (var e in Model)
            {
                <input type="hidden" name="Id" value="@e.Id"/>
                <div class="row mb-3">
                    <div class="col-2">
                        @e.FirstName
                    </div>

                    <div class="col2">
                        @e.LastName
                    </div>

                    <div class="col-5">
                        @e.EmailAddress
                    </div>

                    <div class="col">
                        @e.Quote
                    </div>
                    
                </div>
            }

        </div>
    </div>
</div>